@using Microsoft.AspNetCore.Authentication
@{
    ViewData["Title"] = "Secret Page";
}
<h2>User Info</h2>
<dl>
    <dd>@User.Identity.Name</dd>
</dl>
<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>

    }
</dl>